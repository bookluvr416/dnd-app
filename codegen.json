{
  "overwrite": true,
  "schema": "./src/app/api/graphql/typeDefs.ts",
  "generates": {
    "src/generated/graphql.ts": {
      "plugins": [
        "typescript",
        "typescript-resolvers"
      ],
      "config": {
        "maybeValue": "T | null | undefined",
        "inputMaybeValue": "T | null | undefined"
      }
    }
  },
  "require": ["ts-node/register"]
}